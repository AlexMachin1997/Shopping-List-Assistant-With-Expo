{
  "expo": {
    "name": "shopping-list-app-rewrite",
    "slug": "shopping-list-app-rewrite",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "shopping-list-app-rewrite",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
